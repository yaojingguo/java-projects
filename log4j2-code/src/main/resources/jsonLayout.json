{
  "ts": {
    "$resolver": "timestamp"
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "class": {
    "$resolver": "source",
    "field": "className"
  },
  "method": {
    "$resolver": "source",
    "field": "methodName"
  },
  "line": {
    "$resolver": "source",
    "field": "lineNumber"
  },
  "thread": {
    "$resolver": "thread",
    "field": "name"
  },
  "msg": {
    "$resolver": "message",
    "stringified": true
  },
  "ex": {
    "$resolver": "exception",
    "field": "stackTrace",
    "stackTrace": {
      "stringified": true
    }
  }
}
